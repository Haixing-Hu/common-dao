<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ltd.qubit.commons.dao.mapper">

  <sql id="selectColumnToken">
    ${alias}.`${columnPrefix}value`             AS `${asPrefix}value`,
    ${alias}.`${columnPrefix}create_time`       AS `${asPrefix}create_time`,
    ${alias}.`${columnPrefix}max_age`           AS `${asPrefix}max_age`,
    ${alias}.`${columnPrefix}previous_value`    AS `${asPrefix}previous_value`,
  </sql>

</mapper>
