<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ltd.qubit.commons.dao.mapper">

  <sql id="insertValueSimCard">
    #{${prefix}iccid},
    #{${prefix}imei},
    #{${prefix}meid},
    #{${prefix}phone},
    #{${prefix}operator},
    #{${prefix}country.id},
    <include refid="ltd.qubit.commons.dao.mapper.insertValueLocation">
      <property name="prefix" value="${prefix}location."/>
    </include>
    #{${prefix}networkType},
    #{${prefix}status},
  </sql>

</mapper>
